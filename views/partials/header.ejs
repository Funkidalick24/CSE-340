<header id="top-header">
    <div class="site-header">
        <a href="/" title="Return to home page">
            <img src="/images/site/icon.png" alt="CSE Motors Logo">
        </a>
       <div class="header-links">
        <% if (locals.loggedin) { %>
            <a href="/account/management">Welcome <%= locals.accountData.account_firstname %></a>
            <a href="/account/logout">Logout</a>
          <% } else { %>
            <a href="/account/login" title="Click to log in">My Account</a>
          <% } %>
       </div>
    </div>
</header>

